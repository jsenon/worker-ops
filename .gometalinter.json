{
  "Vendor": true,
  "Deadline": "10m",
  "Sort": [
    "linter",
    "severity",
    "path",
    "line"
  ],
  "EnableGC": true,
  "Aggregate": true,
  "WarnUnmatchedDirective": true,
  "Exclude": [
    "parameter .* always receives",
    ".pb.go",
    ".gen.go",
    "^vendor",
    ".*mocks.*",
    "\\.generated\\."
  ],
  "Enable": [
    "deadcode",
    "gofmt",
    "goimports",
    "golint",
    "ineffassign",
    "interfacer",
    "misspell",
    "unconvert",
    "unparam",
    "vet",
    "errcheck",
    "safesql",
    "gosec",
    "vetshadow",
    "maligned",
    "gocyclo"
  ],
  "LineLength": 220,
  "Cyclo" : 20
  }